{
  "expo": {
    "name": "Zeszycik",
    "slug": "Zeszycik",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./icons/icon.png",
    "splash": {
      "image": "./icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E2395F"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sebagolab.debtnote"
    },
    "android": {
      "softwareKeyboardLayoutMode":"pan",
      "adaptiveIcon": {
        "foregroundImage": "./icons/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.sebagolab.debtnote"
    },
    "web": {
      "favicon": "./icons/favicon.png"
    },
    "newArchEnabled": true,
    "experiments": {
      "baseUrl": "/zeszycik-app"
    }
  }
}
